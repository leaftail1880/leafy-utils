{
  "name": "leafy-utils",
  "version": "6.0.6",
  "description": "Just some of my scripts",
  "main": "exports.js",
  "types": "types/exports.d.ts",
  "author": "leaftail1880",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "node ./build.js && tsc"
  },
  "bin": {
    "commit": "./bin/commit.js",
    "publish": "./bin/publish.js"
  },
  "files": [
    "src",
    "types",
    "exports.js",
    "install.js",
